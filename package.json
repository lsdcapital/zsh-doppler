{
  "name": "zsh-doppler",
  "version": "1.0.0",
  "description": "Zsh plugin to show current Doppler project and config in your prompt",
  "type": "module",
  "scripts": {
    "update": "pnpm up -i -L",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:perf": "vitest run tests/performance.test.js",
    "typecheck": "tsc --noEmit",
    "perf:baseline": "node tests/baseline-manager.js summary",
    "perf:record": "npm run test:perf && echo 'Run with specific values: node tests/baseline-manager.js record <metric> <values>'",
    "perf:check": "vitest run tests/performance.test.js --reporter=verbose"
  },
  "keywords": [
    "zsh",
    "plugin",
    "doppler",
    "prompt",
    "secrets",
    "powerlevel10k"
  ],
  "author": "Stefan Lesicnik <stef@lsd.capital>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lsdcapital/zsh-doppler.git"
  },
  "homepage": "https://github.com/lsdcapital/zsh-doppler#readme",
  "bugs": {
    "url": "https://github.com/lsdcapital/zsh-doppler/issues"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.8",
    "vitest": "^4.0.8"
  },
  "engines": {
    "node": ">=24"
  },
  "packageManager": "pnpm@10.21.0+sha512.da3337267e400fdd3d479a6c68079ac6db01d8ca4f67572083e722775a796788a7a9956613749e000fac20d424b594f7a791a5f4e2e13581c5ef947f26968a40"
}