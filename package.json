{
  "name": "@lsdcapital/zsh-doppler",
  "version": "1.0.1",
  "description": "Zsh plugin to show current Doppler project and config in your prompt",
  "type": "module",
  "scripts": {
    "update": "pnpm up -i -L",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:perf": "vitest run tests/performance.test.js",
    "typecheck": "tsc --noEmit",
    "perf:baseline": "node tests/baseline-manager.js summary",
    "perf:record": "npm run test:perf && echo 'Run with specific values: node tests/baseline-manager.js record <metric> <values>'",
    "perf:check": "vitest run tests/performance.test.js --reporter=verbose",
    "release": "npm version patch && git push --follow-tags",
    "release:patch": "npm version patch && git push --follow-tags",
    "release:minor": "npm version minor && git push --follow-tags",
    "release:major": "npm version major && git push --follow-tags"
  },
  "keywords": [
    "zsh",
    "plugin",
    "doppler",
    "prompt",
    "secrets",
    "powerlevel10k"
  ],
  "author": "Stefan Lesicnik <stef@lsd.capital>",
  "license": "MIT",
  "files": [
    "zsh-doppler.plugin.zsh",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lsdcapital/zsh-doppler.git"
  },
  "homepage": "https://github.com/lsdcapital/zsh-doppler#readme",
  "bugs": {
    "url": "https://github.com/lsdcapital/zsh-doppler/issues"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.17",
    "vitest": "^4.0.17"
  },
  "engines": {
    "node": ">=24"
  },
  "packageManager": "pnpm@10.28.0+sha512.05df71d1421f21399e053fde567cea34d446fa02c76571441bfc1c7956e98e363088982d940465fd34480d4d90a0668bc12362f8aa88000a64e83d0b0e47be48"
}
